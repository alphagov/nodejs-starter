{
  "name": "nodejs-starter",
  "version": "1.0.0",
  "description": "A blank repo with nothing but a linter and formatter configured",
  "main": "index.js",
  "scripts": {
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonheslop/nodejs-starter.git"
  },
  "keywords": [
    "nodejs",
    "js",
    "standardjs"
  ],
  "author": "Jon Heslop",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jonheslop/nodejs-starter/issues"
  },
  "homepage": "https://github.com/jonheslop/nodejs-starter#readme",
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "devDependencies": {
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-promise": "^3.6.0",
    "husky": "^0.14.3",
    "lint-staged": "^5.0.0",
    "standard": "^10.0.3"
  }
}
